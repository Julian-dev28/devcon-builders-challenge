  "description": "OKX Developer Workshop - Build Multi-Chain DApps",
  "scripts": {
    "install-all": "npm run install:widget && npm run install:swap && npm run install:bot",
    "install:widget": "cd dex-widget-demo && npm install",
    "install:swap": "cd evm-swap-app && npm install",
    "install:bot": "cd trading-bot && npm install",
