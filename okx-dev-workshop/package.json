    "start:swap": "cd evm-swap-app && npm run start",
    "start:bot": "cd trading-bot && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "okx",
    "blockchain",
    "dex",
    "trading",
    "workshop"
  ],
